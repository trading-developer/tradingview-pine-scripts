                allConditionTrue  :=  true
        else
                allConditionTrue  :=  false
if  (cb4  ==  true)
        if  (allConditionTrue==true  and  is_sma_50_above_sma_150_and_200==true)
                allConditionTrue  :=  true
        else
                allConditionTrue  :=  false
if  (cb5  ==  true)
