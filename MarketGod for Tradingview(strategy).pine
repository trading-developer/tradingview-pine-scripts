MGT(x,y)=>
        norm=0.0
        sum=0.0
        for  i=0  to  y-1
                weight=(y-i)*y
                norm:=norm+weight
                sum:=sum+x[i]*weight
        sum/norm
macd(source,fastLength,slowLength)=>
        fastMA=ema(source,fastLength)
