x_dev=x-sma(x,t)
y_dev=y-sma(y,t)
xy_sum=0.0
x2_sum=0.0
y2_sum=0.0
fori=0to(t-1)
xy_sum:=xy_sum+x_dev[i]*y_dev[i]
x2_sum:=x2_sum+pow(x_dev[i],2)
y2_sum:=y2_sum+pow(y_dev[i],2)
xy_sum/sqrt(x2_sum*y2_sum)
