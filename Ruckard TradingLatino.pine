dirmov(len)=>
up=change(high)
down=-change(low)
truerange=rma(tr,len)
plus=fixnan(100*rma(up>downandup>0?up:0,len)/truerange)
minus=fixnan(100*rma(down>upanddown>0?down:0,len)/truerange)
[plus,minus]
adx(dilen,adxlen)=>
[plus,minus]=dirmov(dilen)
sum=plus+minus
adx=100*rma(abs(plus-minus)/(sum==0?1:sum),adxlen)
[adx,plus,minus]
