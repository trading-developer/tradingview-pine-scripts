//Linearregression-functions
lrp_Sum(src,p)=>a=cum(src),a-a[max(p,0)]
lrp_Wma(src,p)=>
mp=max(p,0)
denom=mp*(mp+1)/2
a=cum(src),(mp*a-lrp_Sum(a[1],p))/denom
lrp_getAandB(src,p)=>
a=lrp_Wma(src[1],p)
b=lrp_Sum(src[1],p)/p
A=4*b-3*a
B=3*a-2*b
[A,B]
fun_linear_regression_points(src,periods_number)=>
