booltimeFilterApproval=dateFilterApprovalandsessionFilterApproval
emaLine=request.security(symbol=syminfo.tickerid,timeframe=emaTimeframe,expression=ta.ema(emaSrc,emaLength),gaps=barmerge.gaps_off)
